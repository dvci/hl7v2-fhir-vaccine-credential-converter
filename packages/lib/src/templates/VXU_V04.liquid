{
    "resourceType": "Bundle",
    "id": "bundle-example",
    "type": "batch",
    "entry": [
        {% if PID %}
        {% include 'PID' with PID %}
        {% endif %}
        {% for order in ORDER %}
            {% if forloop.first == true %}
                {% if PID %},{% endif %}
            {% else %},{% endif %}
            {% include 'RXA' with order.RXA %}
        {% endfor %}
    ]
}        